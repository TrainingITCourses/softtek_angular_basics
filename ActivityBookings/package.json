    "@angular/platform-server": "^17.3.0",
    "@angular/router": "^17.3.0",
    "@angular/ssr": "^17.3.0",
    "@picocss/pico": "^2.0.6",
    "express": "^4.18.3",
    "rxjs": "~7.8.1",
